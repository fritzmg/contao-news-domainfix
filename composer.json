{
	"name":"fritzmg/contao-news-domainfix",
	"description":"Small Contao extension that fixes news target links for pages that are on a different domain or language.",
	"keywords":["contao", "news", "internal", "domain"],
	"type":"contao-module",
	"homepage":"http://www.inspiredminds.at",
	"license":"GPL-2.0",
	"authors":[
		{
			"name":"Fritz Michael Gschwantner",
			"homepage":"http://www.inspiredminds.at",
			"email":"fmg@inspiredminds.at",
			"role":"Developer"
		}
	],
	"support":{
		"email":"fmg@inspiredminds.at",
		"issues":"https://github.com/fritzmg/contao-news-domainfix/issues",
		"source":"https://github.com/fritzmg/contao-news-domainfix",
		"forum":"https://community.contao.org/de"
	},
	"require":{
		"php":">=5.2",
		"contao/core":"~3.0",
		"contao-community-alliance/composer-installer":"*"
	},
	"replace": {
		"contao-legacy/news_domainfix": "self.version"
	},
	"extra":{
		"contao":{
			"sources":{
				"system/modules/news_domainfix":"system/modules/news_domainfix"
			}
		}
	}
}